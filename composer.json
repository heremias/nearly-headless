    "pantheon-systems/quicksilver-pushback": "^2",
    "phpunit/phpunit": "^8.4"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "build-assets": [
      "@prepare-for-pantheon",
      "composer install --optimize-autoloader",
      "npm --prefix web/profiles/custom/osu_standard/themes/standard_patterns ci --no-audit",
      "npm run build"
    ],
    "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
    "code-sniff": "echo 'No code sniff step defined.'",
    "unit-test": "echo 'No unit test step defined.'",
    "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "npm ci"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-create-project-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/modules/osu/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/profiles/osu/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "web/themes/osu/{$name}": [
        "type:drupal-custom-theme"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/core": {
        "fix js errors when editing formats: https://www.drupal.org/node/2556069": "https://www.drupal.org/files/issues/2020-10-09/2556069-62.patch",
        "fix ajax pagers": "https://www.drupal.org/files/issues/2020-11-29/2858890-58.patch"
      },
      "drupal/allowed_formats": {
        "default format missing": "https://www.drupal.org/files/issues/allowed_formats-missing_default_value.patch"
      },
      "drupal/datalayer":{
        "path alias manager d9 https://www.drupal.org/project/datalayer/issues/3194726": "https://www.drupal.org/files/issues/2020-09-18/3171896-path_alias-manager.patch"
      },
      "drupal/migrate_files": {
        "does not handle public files": "https://www.drupal.org/files/issues/2018-10-20/destination_support_localUri-3008045-2.patch"
      }
    },
    "drupal-scaffold": {
      "allowed-packages": [
        "pantheon-systems/drupal-integrations"
      ],
      "locations": {
        "web-root": "./web"
      },
      "file-mapping": {
        "[project-root]/.editorconfig": false,
        "[web-root]/sites/default/default.settings.php": false
      }
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "secure-http": false,
    "platform": {
      "php": "7.4"
    }
  }
}
