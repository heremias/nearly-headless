<div class="o-agenda-timeslot row no-gutters">
  <div class="col-md-3">
    <h3 class="o-agenda-timeslot--heading"> {{ title }} </h3>
  </div>
  <div class="o-agenda-timeslot--rows col-md-9">
    {% for row in items %}
      <div {{ row.attributes.addClass("o-agenda-timeslot--row") }}>
        {{ row.content }}
      </div>
    {% endfor %}
  </div>
</div>
